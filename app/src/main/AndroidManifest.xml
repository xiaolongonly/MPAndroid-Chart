<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="cn.xiaolongonly.mpchartsample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.LineChartActivity1"/>
        <activity android:name=".ui.LineChartActivity2"/>
        <activity android:name=".ui.BarChartActivity"/>
        <activity android:name=".ui.HorizontalBarChartActivity"/>
        <activity android:name=".ui.PieChartActivity"/>
        <activity android:name=".ui.RadarChartActivty"/>
    </application>

</manifest>